{
  "name": "starter",
  "version": "1.0.0",
  "description": "Starter Project",
  "main": "index.js",
  "scripts": {
    "assets": "babel-node ./src/scripts/assets.js",
    "build": "NODE_ENV=production babel-node ./src/scripts/build.js",
    "dev": "babel-node ./src/scripts/dev.js",
    "deploy": "babel-node ./src/scripts/deploy.js",
    "g": "babel-node ./src/scripts/generate/index.js",
    "knex": "babel-node ./src/scripts/knex.js",
    "test": "NODE_ENV=test babel-node ./src/scripts/test.js",
    "desktop:dmgbackground": "tiffutil -cathidpicheck ./build/screens/dmg.png ./build/screens/dmg@2x.png -out ./build/screens/dmg.tiff",
    "desktop:build": "build --mac --win --x64 --project=./src/desktop --config=./config/config.json",
    "desktop:start": "electron ./src/desktop/www",
    "mobile:ios:simulator": "cd ./src/mobile && cordova run ios --buildFlag='-UseModernBuildSystem=0' --target=iPhone-8",
    "mobile:ios:device": "cd ./src/mobile && cordova run ios --buildFlag='-UseModernBuildSystem=0'",
    "mobile:ios:package": "cd ./src/mobile && cordova build ios --release --device --buildFlag='-UseModernBuildSystem=0' --packageType='app-store'",
    "mobile:android": "cd ./src/mobile && cordova run android --release --device"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
